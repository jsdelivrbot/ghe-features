{
  "manifest_version": 2,
  "name": "ghe-features",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/blog/*"],
      "js": ["ghe-features.js"]
    }
  ],
  "permissions": [
    "https://github.com/*",
    "https://rawgit.com/*",
    "https://cdn.rawgit.com/*"
  ]
}